<Window x:Class="Eksisterende_Kode.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Eksisterende_Kode"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <WrapPanel HorizontalAlignment="Center" VerticalAlignment="Center" Margin="-15,10,28.6,380">
            <Button x:Name="OpretBooking" Content="Opret Booking" Width="95" Height="18" Click="btn_OpretBooking_Click"/>
            <Button x:Name="RedigerBooking" Content="Rediger Booking" Width="95" Height="18" Click="btn_RedigerBooking_Click"/>
            <Button x:Name="SletBooking" Content="Slet Booking" Width="95" Height="18" Click="btn_SletBooking_Click"/>
            <Button x:Name="TilføjRustvogn" Content="Tilføj Rustvogn" Width="95" Height="18" Click="btn_TilføjRustvogn_Click"/>
            <Button x:Name="RedigerRustvogn" Content="Rediger Rustvogn" Width="95" Height="18" Click="btn_RedigerRustvogn_Click"/>
            <Button x:Name="AfslutProgram" Content="Afslut Program" Width="95" Height="18" Click="btn_AfslutProgram_Click"/>
        </WrapPanel>
        <Frame Name="ContentFrame" Margin="190,100,25,25" 
               NavigationUIVisibility="Hidden"/>



    </Grid>
</Window>
