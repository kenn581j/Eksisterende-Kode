<Page x:Class="Eksisterende_Kode.OpretBooking"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Eksisterende_Kode"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="OpretBooking">

    <Grid Margin="0,27,38,24">
        <StackPanel Margin="20,10,0,20" HorizontalAlignment="Left" Width="237">
            <TextBox Height="23" TextWrapping="Wrap" Width="180" Text="Vælg starttidspunkt"/>
            <TextBox Height="23" TextWrapping="Wrap" Width="180" Text="Vælg sluttidspunkt"/>
            <TextBox Height="23" TextWrapping="Wrap" Width="180" Text="Skriv addresse"/>
            <TextBox Height="23" TextWrapping="Wrap" Width="180" Text="Skriv en evt. kommentar"/>
            <TextBlock TextWrapping="Wrap" Width="180" Background="White"><Run Text="Ønskes rustvogn?"/><InlineUIContainer>
                    <CheckBox Content="Ja"  Background="White" Width="180"/>


                </InlineUIContainer></TextBlock>
            <Button Name="AfslutBooking" Width="180" Content="Afslut og gem Booking"/>


        </StackPanel>
    </Grid>
</Page>
