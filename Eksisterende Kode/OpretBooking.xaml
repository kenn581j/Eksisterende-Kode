<Page x:Class="Eksisterende_Kode.OpretBooking"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Eksisterende_Kode"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="OpretBooking">

    <Grid>
        <StackPanel Margin="20, 10, 600, 20">
            <TextBox Height="23" TextWrapping="Wrap" Text="Vælg starttidspunkt"/>
            <TextBox Height="23" TextWrapping="Wrap" Text="Vælg sluttidspunkt"/>
            <TextBox Height="23" TextWrapping="Wrap" Text="Skriv addresse"/>
            <TextBox Height="23" TextWrapping="Wrap" Text="Skriv en evt. kommentar"/>
            <TextBlock TextWrapping="Wrap" Background="White"><Run Text="Ønskes rustvogn?"/><InlineUIContainer>
                    <CheckBox Content="Ja"  Background="White" Width="180"/>


                </InlineUIContainer></TextBlock>
            <Button Name="AfslutBooking" Content="Afslut og gem Booking"/>


        </StackPanel>
    </Grid>
</Page>
